{
    "name":        "webino/webino-image-thumb",
    "description": "Image Thumbnailer for Zend Framework 2",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "keywords":    ["webino", "image", "thumb", "zf2", "zend", "framework"],
    "homepage":    "https://github.com/webino/WebinoImageThumb",
    "authors": [
        {
            "name": "Webino, s. r. o.",
            "email": "info@webino.sk",
            "homepage": "http://webino.sk",
            "role": "Developer"
        },
        {
            "name": "Peter Bačinský",
            "email": "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "masterexploder/PHPThumb": "2.*"
    },
    "suggest": {
        "zendframework/zend-modulemanager": "To use it as a Zend Framework module"
    },
    "require-dev": {
        "element-34/php-webdriver": "1.*",
        "mayflower/php-codebrowser": "*",
        "pdepend/pdepend": "1.*",
        "phpdocumentor/phpdocumentor": "2.*",
        "phploc/phploc": "*",
        "phpmd/phpmd": "1.*",
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "0.*",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "1.*",
        "zendframework/zendframework": "2.*"
    },
    "autoload": {
        "psr-0": {"WebinoImageThumb": "src/"}
    }
}
